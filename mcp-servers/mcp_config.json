            "args": [
                "/home/metaspeekoj/mcp-servers/test_file_manager_mcp_server.js"
            ],
            "cwd": "/home/metaspeekoj/mcp-servers",
            "env": {
                "NODE_PATH": "/home/metaspeekoj/node_modules"
            }
        }
    }
