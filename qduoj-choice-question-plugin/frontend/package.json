{
  "name": "qduoj-choice-question-plugin",
  "version": "1.0.0",
  "description": "QDUOJ选择题插件前端",
  "main": "plugin-entry.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "serve": "webpack serve --mode=development",
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "eslint src --ext .js,.vue --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist"
  },
  "keywords": [
    "qduoj",
    "plugin",
    "choice-question",
    "vue"
  ],
  "author": "QDUOJ Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "echarts": "^5.4.0",
    "element-ui": "^2.0.0",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.6.0",
    "katex": "^0.16.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "tar-simditor": "^3.0.5",
    "vue": "^2.6.0",
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "^2.4.6",
    "babel-jest": "^30.1.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^30.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.10.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QingdaoU/OnlineJudge.git"
  },
  "bugs": {
    "url": "https://github.com/QingdaoU/OnlineJudge/issues"
  },
  "homepage": "https://github.com/QingdaoU/OnlineJudge#readme",
  "peerDependencies": {
    "element-ui": "^2.0.0",
    "vue": "^2.6.0",
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!**/node_modules/**"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "warn",
      "no-debugger": "warn",
      "vue/no-unused-vars": "warn",
      "vue/require-v-for-key": "error",
      "vue/no-use-v-if-with-v-for": "error"
    }
  }
}
